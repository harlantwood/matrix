{
  "name": "ci-matrix",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "check": "svelte-check --fail-on-warnings --tsconfig ./tsconfig.json",
    "check:watch": "yarn check --watch",
    "check-ts": "tsc --noEmit",
    "lint": "yarn _prettier --write && yarn _eslint --fix && yarn check",
    "lint:ci": "yarn _prettier --check && yarn _eslint && yarn check",
    "_prettier": "prettier --plugin-search-dir=. .",
    "_eslint": "eslint --max-warnings 0 --ignore-path .gitignore .",
    "deploy": "gh-pages --dist build",
    "sort": "sort-package-json"
  },
  "dependencies": {
    "octokit": "^1.1.0",
    "svelte-table": "^0.1.17"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.13",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "faker": "^5.5.3",
    "gh-pages": "^3.2.3",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "sort-package-json": "^1.50.0",
    "svelte": "^3.34.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  }
}
